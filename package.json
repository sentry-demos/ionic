{
  "name": "ionic-conference-app",
  "version": "0.0.0",
  "description": "Ionic Conference App",
  "author": "Ionic Team <hi@ionicframework.com>",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node config-env.ts",
    "start": "ng serve",
    "build": "ng build && npm run sentry:sourcemaps",
    "build:prod": "ng build --configuration=production",
    "build:prod:sentry": "ng build --configuration=production && npx sentry-cli releases files upload-sourcemaps www --dist-tags=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "webdriver-manager update --standalone false --gecko false",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org team-se --project simon-capacitor www/ && sentry-cli sourcemaps upload --org team-se --project simon-capacitor www/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic-conference-app.git"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/pwa": "^14.2.0",
    "@angular/router": "^14.3.0",
    "@angular/service-worker": "^14.3.0",
    "@capacitor/android": "^4.8.1",
    "@capacitor/cli": "^4.8.1",
    "@capacitor/core": "^4.8.1",
    "@ionic-native/core": "^5.36.0",
    "@ionic-native/in-app-browser": "^5.36.0",
    "@ionic-native/splash-screen": "^5.36.0",
    "@ionic-native/status-bar": "^5.36.0",
    "@ionic/angular": "^7.0.0",
    "@ionic/storage": "^3.0.6",
    "@ionic/storage-angular": "^4.0.0",
    "@sentry/angular": "8.9.2",
    "@sentry/capacitor": "^1.0.0-beta.1",
    "cordova-android": "^10.1.2",
    "cordova-ios": "^6.3.0",
    "cordova-plugin-device": "^2.1.0",
    "cordova-plugin-inappbrowser": "^5.0.0",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-splashscreen": "^6.0.2",
    "cordova-plugin-statusbar": "^3.0.0",
    "core-js": "^3.30.0",
    "dotenv": "^16.3.1",
    "fs": "0.0.1-security",
    "rxjs": "^7.8.1",
    "sentry-cordova": "^0.17.0",
    "sw-toolbox": "3.6.0",
    "tslib": "^2.6.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1402.13",
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular-devkit/core": "^14.2.13",
    "@angular-devkit/schematics": "^14.2.13",
    "@angular/cli": "^14.2.13",
    "@angular/compiler": "^14.3.0",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@ionic/angular-toolkit": "^7.0.0",
    "@ionic/lab": "^3.0.0",
    "@sentry/cli": "^2.53.0",
    "@types/jasmine": "^4.3.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^18.15.0",
    "codelyzer": "^6.0.2",
    "jasmine-core": "^4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "^3.0.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "protractor": "^7.0.0",
    "puppeteer": "^19.0.0",
    "ts-node": "^10.9.0",
    "tslint": "^6.1.3",
    "typescript": "~4.7.4"
  },
  "cordova": {
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": {
      "cordova-plugin-device": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-ionic-keyboard": {}
    }
  },
  "config": {
    "ionic_generate_source_map": "true"
  }
}